# Celery EC2 서버용 환경 변수
# 사용법: .env.celery 로 복사 후 값 채우기

# ECR 정보
ECR_REGISTRY=123456789012.dkr.ecr.ap-northeast-2.amazonaws.com
IMAGE_TAG=latest

# Django 설정
DJANGO_SECRET_KEY=your-secret-key-here
DEBUG=False

# Django EC2 Private IP (RabbitMQ 연결용)
DJANGO_EC2_PRIVATE_IP=10.0.1.100

# AWS RDS 정보 (Django EC2와 동일)
DB_HOST=hadbetter-db.xxxxxxxxxx.ap-northeast-2.rds.amazonaws.com
DB_PORT=5432
DB_NAME=hadbetter
DB_USER=postgres
DB_PASSWORD=your-db-password

# Redis (Django EC2의 Private IP 사용)
REDIS_URL=redis://10.0.1.100:6379/0

# RabbitMQ (Django EC2에 있음)
RABBITMQ_HOST=10.0.1.100
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASS=your-rabbitmq-password
RABBITMQ_PASSWORD=your-rabbitmq-password

# External APIs
TMAP_API_KEY=your-tmap-api-key
MAPBOX_ACCESS_TOKEN=your-mapbox-token
BUS_API_KEY=your-bus-api-key
SUBWAY_API_KEY=your-subway-api-key

# Logging
DJANGO_LOG_LEVEL=INFO
